{
  "author": "Dario Sestero",
  "name": "dsestero-mediawiki",
  "version": "2.1.0",
  "license": "Apache-2.0",
  "summary": "Puppet module for multi-tenant MediaWiki installation.",
  "project_page": "https://github.com/dsestero/puppet-mediawiki",
